<Window x:Class="MainWindow" x:Name="ui_bus_rode_main_window"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Title="bus_rode 8" Height="600" Width="800" FontFamily="Microsoft YaHei UI Light" WindowStyle="None" 
        AllowsTransparency="True" MinHeight="600" MinWidth="800" TextOptions.TextFormattingMode="Display" 
        Icon="bus_rode_icon.ico" x:FieldModifier="Public">
    <!-- 要阴影在上面加 BorderThickness="10"-->
    <Window.Background>
        <SolidColorBrush Color="#FF2D2D2D"/>
    </Window.Background>
    <!-- 要阴影取消注释，但是界面会出现一点问题，为了专注性，不加阴影-->
    <!--<Window.Effect>
        <DropShadowEffect BlurRadius="10" Color="#FF858484" Direction="90" ShadowDepth="3" />
    </Window.Effect>-->

    <!--主窗体标题与客户区-->
    <Grid x:Name="ui_main_user_area">
        <Grid.RowDefinitions >
            <!-- 标题-->
            <RowDefinition Height="30"/>
            <!-- 客户区-->
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>

        <!-- 标题-->
        <Grid Grid.Row="0" MouseDown="window_move">
            <!-- 背景颜色-->
            <Grid.Background>
                <SolidColorBrush x:Name="ui_color_4" Color="#8f0000ff"/>
            </Grid.Background>

            <!-- 标题栏分区-->
            <Grid.ColumnDefinitions >
                <!-- 2个功能按钮-->
                <ColumnDefinition Width="100"/>
                <ColumnDefinition Width="100"/>

                <!-- 标题文字-->
                <ColumnDefinition Width="*"/>
                <ColumnDefinition Width="Auto"/>
                <ColumnDefinition Width="*"/>

                <!-- 时间-->
                <ColumnDefinition Width=" Auto"/>

                <!-- 消息中心-->
                <ColumnDefinition Width="15"/>
                <ColumnDefinition x:Name="ui_title_msg_column_1" Width="30"/>
                <ColumnDefinition x:Name="ui_title_msg_column_2" Width="Auto"/>
                <ColumnDefinition x:Name="ui_title_msg_column_3" Width="15"/>

                <!-- 最小化 最大化 关闭-->
                <ColumnDefinition Width="30"/>
                <ColumnDefinition Width="30"/>
                <ColumnDefinition Width="30"/>
            </Grid.ColumnDefinitions>

            <Grid Grid.Column="0" x:Name="ui_title_back_btn">
                <Grid.Background >
                    <SolidColorBrush x:Name="ui_title_back_btn_color" Color="#b48f8f8f"/>
                </Grid.Background>

                <Grid.ColumnDefinitions >
                    <ColumnDefinition Width="*"/>
                    <ColumnDefinition Width="30"/>
                    <ColumnDefinition Width="*"/>
                </Grid.ColumnDefinitions>

                <Grid Grid.Column="1">
                    <Grid.Background >
                        <ImageBrush ImageSource="photo\left.png" />
                    </Grid.Background>
                </Grid>
            </Grid>

            <Grid Grid.Column="1" x:Name="ui_title_menu_btn">
                <Grid.Background>
                    <SolidColorBrush x:Name="ui_title_menu_btn_color" Color="#b48f8f8f"/>
                </Grid.Background>

                <Grid.ColumnDefinitions >
                    <ColumnDefinition Width="*"/>
                    <ColumnDefinition Width="30"/>
                    <ColumnDefinition Width="*"/>
                </Grid.ColumnDefinitions>

                <Grid Grid.Column="1">
                    <Grid.Background >
                        <ImageBrush ImageSource="photo\menu.png" />
                    </Grid.Background>
                </Grid>
            </Grid>

            <Grid Grid.Column="3">
                <Grid.ColumnDefinitions >
                    <ColumnDefinition Width="*"/>
                    <ColumnDefinition Width="Auto"/>
                    <ColumnDefinition Width="*"/>
                </Grid.ColumnDefinitions>
                <Grid.RowDefinitions>
                    <RowDefinition Height="*"/>
                    <RowDefinition Height="Auto"/>
                    <RowDefinition Height=" *"/>
                </Grid.RowDefinitions>

                <TextBlock x:Name="ui_window_title" Grid.Column="1" Grid.Row="1" Text="bus_rode" FontSize="15" Foreground="white"/>
            </Grid>

            <Grid Grid.Column="5">
                <Grid.ColumnDefinitions >
                    <ColumnDefinition Width="*"/>
                    <ColumnDefinition Width="Auto"/>
                    <ColumnDefinition Width="*"/>
                </Grid.ColumnDefinitions>
                <Grid.RowDefinitions>
                    <RowDefinition Height="*"/>
                    <RowDefinition Height="Auto"/>
                    <RowDefinition Height=" *"/>
                </Grid.RowDefinitions>

                <TextBlock x:Name="ui_title_time" Grid.Column="1" Grid.Row="1" Text="00:00" FontSize="15" Foreground="white"/>
            </Grid>

            <Grid Grid.Column="7" MouseDown="ui_function_message">
                <Grid.Background>
                    <ImageBrush ImageSource="photo\message.png"/>
                </Grid.Background>
            </Grid>

            <Grid Grid.Column="8">
                <Grid.ColumnDefinitions >
                    <ColumnDefinition Width="*"/>
                    <ColumnDefinition Width="Auto"/>
                    <ColumnDefinition Width="*"/>
                </Grid.ColumnDefinitions>
                <Grid.RowDefinitions>
                    <RowDefinition Height="*"/>
                    <RowDefinition Height="Auto"/>
                    <RowDefinition Height=" *"/>
                </Grid.RowDefinitions>

                <TextBlock x:Name="ui_title_msg_count" Grid.Column="1" Grid.Row="1" Text="0" FontSize="15" Foreground="white"/>
            </Grid>

            <Grid Grid.Column="10" MouseDown="window_behind">
                <Grid.Background>
                    <ImageBrush ImageSource="photo\behind.png"/>
                </Grid.Background>
            </Grid>
            <Grid Grid.Column="11" x:Name="ui_title_max_btn">
                <Grid.Background>
                    <ImageBrush ImageSource="photo\max.png"/>
                </Grid.Background>
            </Grid>
            <Grid Grid.Column="12" MouseDown="window_exit">
                <Grid.Background>
                    <ImageBrush ImageSource="photo\close.png"/>
                </Grid.Background>
            </Grid>

        </Grid>

        <!-- 客户区-->
        <Canvas Grid.Row="1" Background="#5f000000" x:Name="ui_user_area">

            <!-- 开始界面-->
            <Grid x:Name="ui_form_start" Width="800" Height="570" Canvas.Top="0" Canvas.Left="0">
                <Grid.ColumnDefinitions >
                    <ColumnDefinition Width="*"/>
                    <ColumnDefinition Width="50"/>
                    <ColumnDefinition Width="3*"/>
                </Grid.ColumnDefinitions>

                <!-- 界面按钮-->
                <Grid Grid.Column="0">
                    <Grid.RowDefinitions >
                        <RowDefinition Height="*"/>
                        <RowDefinition Height="*"/>
                        <RowDefinition Height="*"/>
                        <RowDefinition Height="*"/>
                        <RowDefinition Height="*"/>
                        <RowDefinition Height="*"/>
                    </Grid.RowDefinitions>

                    <Grid Grid.Row="1" Grid.RowSpan="4" HorizontalAlignment="Center">
                        <Grid.RowDefinitions >
                            <RowDefinition Height="*"/>
                            <RowDefinition Height="*"/>
                            <RowDefinition Height="*"/>
                            <RowDefinition Height="*"/>
                        </Grid.RowDefinitions>

                        <!-- 线路-->
                        <Grid Grid.Row="0" HorizontalAlignment="Left">
                            <Grid.ColumnDefinitions >
                                <ColumnDefinition Width="auto"/>
                                <ColumnDefinition Width="*"/>
                            </Grid.ColumnDefinitions>

                            <TextBlock Grid.Column="1" Foreground="White" FontSize="20" Text="{DynamicResource lang_form_start_btn_line}" TextAlignment="Center" VerticalAlignment="Center" HorizontalAlignment="Left" Margin="10,0,0,0"/>

                            <Grid Grid.Column="0" x:Name="ui_form_start_btn_1_goto" Width="40" Height="40">

                                <Ellipse Stroke="white" Width="40" Height="40" HorizontalAlignment="Left">
                                    <Ellipse.Fill>
                                        <SolidColorBrush x:Name="ui_color_5" Color="#6f0000ff"/>
                                    </Ellipse.Fill>
                                </Ellipse>

                                <Grid Width="40" Height="40" HorizontalAlignment="Left" >
                                    <Grid.Background>
                                        <ImageBrush ImageSource="photo\line.png"/>
                                    </Grid.Background>
                                </Grid>

                                <!-- 动画-->
                                <Grid.Effect >
                                    <DropShadowEffect x:Name="ui_contorl_btn_light_11" BlurRadius="40" Color="#FFffffff" Direction="0" ShadowDepth="0" Opacity="0"/>
                                </Grid.Effect>
                                <Grid.Triggers>
                                    <EventTrigger RoutedEvent="MouseEnter">
                                        <BeginStoryboard >
                                            <Storyboard >
                                                <DoubleAnimation Storyboard.TargetName="ui_contorl_btn_light_11" Storyboard.TargetProperty="Opacity" From="0" To="1" Duration="0:0:0.2"/>
                                            </Storyboard>
                                        </BeginStoryboard>
                                    </EventTrigger>
                                    <EventTrigger RoutedEvent="MouseLeave">
                                        <BeginStoryboard >
                                            <Storyboard >
                                                <DoubleAnimation Storyboard.TargetName="ui_contorl_btn_light_11" Storyboard.TargetProperty="Opacity" From="1" To="0" Duration="0:0:0.2"/>
                                            </Storyboard>
                                        </BeginStoryboard>
                                    </EventTrigger>


                                </Grid.Triggers>

                            </Grid>
                        </Grid>

                        <!-- 车站-->
                        <Grid Grid.Row="1" HorizontalAlignment="Left">
                            <Grid.ColumnDefinitions >
                                <ColumnDefinition Width="auto"/>
                                <ColumnDefinition Width="*"/>
                            </Grid.ColumnDefinitions>

                            <TextBlock Grid.Column="1" Foreground="White" FontSize="20" Text="{DynamicResource lang_form_start_btn_stop}" TextAlignment="Center" VerticalAlignment="Center" HorizontalAlignment="Left" Margin="10,0,0,0"/>
                            <Grid Grid.Column="0" x:Name="ui_form_start_btn_2_goto" Width="40" Height="40">

                                <Ellipse Stroke="white" Width="40" Height="40">
                                    <Ellipse.Fill>
                                        <SolidColorBrush x:Name="ui_color_6" Color="#6f0000ff"/>
                                    </Ellipse.Fill>
                                </Ellipse>

                                <Grid Width="40" Height="40">
                                    <Grid.Background>
                                        <ImageBrush ImageSource="photo\stop.png"/>
                                    </Grid.Background>
                                </Grid>


                                <!-- 动画-->
                                <Grid.Effect >
                                    <DropShadowEffect x:Name="ui_contorl_btn_light_12" BlurRadius="40" Color="#FFffffff" Direction="0" ShadowDepth="0" Opacity="0"/>
                                </Grid.Effect>
                                <Grid.Triggers>
                                    <EventTrigger RoutedEvent="MouseEnter">
                                        <BeginStoryboard >
                                            <Storyboard >
                                                <DoubleAnimation Storyboard.TargetName="ui_contorl_btn_light_12" Storyboard.TargetProperty="Opacity" From="0" To="1" Duration="0:0:0.2"/>
                                            </Storyboard>
                                        </BeginStoryboard>
                                    </EventTrigger>
                                    <EventTrigger RoutedEvent="MouseLeave">
                                        <BeginStoryboard >
                                            <Storyboard >
                                                <DoubleAnimation Storyboard.TargetName="ui_contorl_btn_light_12" Storyboard.TargetProperty="Opacity" From="1" To="0" Duration="0:0:0.2"/>
                                            </Storyboard>
                                        </BeginStoryboard>
                                    </EventTrigger>


                                </Grid.Triggers>

                            </Grid>


                        </Grid>

                        <!-- 设置-->
                        <Grid Grid.Row="2" HorizontalAlignment="Left">
                            <Grid.ColumnDefinitions >
                                <ColumnDefinition Width="auto"/>
                                <ColumnDefinition Width="*"/>
                            </Grid.ColumnDefinitions>

                            <TextBlock Grid.Column="1" Foreground="White" FontSize="20" Text="{DynamicResource lang_form_start_btn_contorl}" TextAlignment="Center" VerticalAlignment="Center" HorizontalAlignment="Left" Margin="10,0,0,0"/>
                            <Grid Grid.Column="0" x:Name="ui_form_start_btn_3_goto" Width="40" Height="40">

                                <Ellipse Stroke="white" Width="40" Height="40">
                                    <Ellipse.Fill>
                                        <SolidColorBrush x:Name="ui_color_7" Color="#6f0000ff"/>
                                    </Ellipse.Fill>
                                </Ellipse>

                                <Grid Width="40" Height="40">
                                    <Grid.Background>
                                        <ImageBrush ImageSource="photo\contorl.png"/>
                                    </Grid.Background>
                                </Grid>


                                <!-- 动画-->
                                <Grid.Effect >
                                    <DropShadowEffect x:Name="ui_contorl_btn_light_13" BlurRadius="40" Color="#FFffffff" Direction="0" ShadowDepth="0" Opacity="0"/>
                                </Grid.Effect>
                                <Grid.Triggers>
                                    <EventTrigger RoutedEvent="MouseEnter">
                                        <BeginStoryboard >
                                            <Storyboard >
                                                <DoubleAnimation Storyboard.TargetName="ui_contorl_btn_light_13" Storyboard.TargetProperty="Opacity" From="0" To="1" Duration="0:0:0.2"/>
                                            </Storyboard>
                                        </BeginStoryboard>
                                    </EventTrigger>
                                    <EventTrigger RoutedEvent="MouseLeave">
                                        <BeginStoryboard >
                                            <Storyboard >
                                                <DoubleAnimation Storyboard.TargetName="ui_contorl_btn_light_13" Storyboard.TargetProperty="Opacity" From="1" To="0" Duration="0:0:0.2"/>
                                            </Storyboard>
                                        </BeginStoryboard>
                                    </EventTrigger>


                                </Grid.Triggers>


                            </Grid>

                        </Grid>

                        <!-- 关于-->
                        <Grid Grid.Row="3" HorizontalAlignment="Left">
                            <Grid.ColumnDefinitions >
                                <ColumnDefinition Width="auto"/>
                                <ColumnDefinition Width="*"/>
                            </Grid.ColumnDefinitions>

                            <TextBlock Grid.Column="1" Foreground="White" FontSize="20" Text="{DynamicResource lang_form_start_btn_about}" TextAlignment="Center" VerticalAlignment="Center" HorizontalAlignment="Left" Margin="10,0,0,0"/>
                            <Grid Grid.Column="0" x:Name="ui_form_start_btn_4_goto" Width="40" Height="40">

                                <Ellipse Stroke="white" Width="40" Height="40">
                                    <Ellipse.Fill>
                                        <SolidColorBrush x:Name="ui_color_8" Color="#6f0000ff"/>
                                    </Ellipse.Fill>
                                </Ellipse>

                                <Grid Width="40" Height="40">
                                    <Grid.Background>
                                        <ImageBrush ImageSource="photo\about.png"/>
                                    </Grid.Background>
                                </Grid>


                                <!-- 动画-->
                                <Grid.Effect >
                                    <DropShadowEffect x:Name="ui_contorl_btn_light_14" BlurRadius="40" Color="#FFffffff" Direction="0" ShadowDepth="0" Opacity="0"/>
                                </Grid.Effect>
                                <Grid.Triggers>
                                    <EventTrigger RoutedEvent="MouseEnter">
                                        <BeginStoryboard >
                                            <Storyboard >
                                                <DoubleAnimation Storyboard.TargetName="ui_contorl_btn_light_14" Storyboard.TargetProperty="Opacity" From="0" To="1" Duration="0:0:0.2"/>
                                            </Storyboard>
                                        </BeginStoryboard>
                                    </EventTrigger>
                                    <EventTrigger RoutedEvent="MouseLeave">
                                        <BeginStoryboard >
                                            <Storyboard >
                                                <DoubleAnimation Storyboard.TargetName="ui_contorl_btn_light_14" Storyboard.TargetProperty="Opacity" From="1" To="0" Duration="0:0:0.2"/>
                                            </Storyboard>
                                        </BeginStoryboard>
                                    </EventTrigger>


                                </Grid.Triggers>


                            </Grid>

                        </Grid>

                    </Grid>


                </Grid>

                <!-- 提醒-->
                <Grid x:Name="ui_form_start_notice" Grid.Column="2" Margin="0,40,40,40" Opacity="1">

                    <Grid.RowDefinitions >
                        <RowDefinition Height="auto"/>
                        <RowDefinition Height="auto"/>
                        <RowDefinition Height="*"/>
                    </Grid.RowDefinitions>

                    <Grid Grid.Row="0" Margin="0,0,0,10" HorizontalAlignment="Left" VerticalAlignment="Center">
                        <Grid.ColumnDefinitions >
                            <ColumnDefinition Width="40"/>
                            <ColumnDefinition Width="*"/>
                        </Grid.ColumnDefinitions>

                        <Grid Width="40" Height="40">
                            <Grid.Background>
                                <ImageBrush ImageSource="photo\ring.png"/>
                            </Grid.Background>
                        </Grid>

                        <TextBlock Margin="10,0,0,0" Text="{DynamicResource lang_form_start_ring}" Grid.Column="1" HorizontalAlignment="Left" VerticalAlignment="Center" Foreground="White" FontSize="20"/>

                    </Grid>

                    <StackPanel Grid.Row="0" Margin="0,0,0,10" HorizontalAlignment="Right" VerticalAlignment="Center" Orientation="Horizontal">

                        <StackPanel Orientation="Vertical" MouseDown="ui_form_start_select_line">
                            <Grid Width="30" Height="30">
                                <Grid.Background>
                                    <ImageBrush ImageSource="photo\add.png"/>
                                </Grid.Background>
                            </Grid>

                            <TextBlock Grid.Row="2" Text="{DynamicResource lang_form_start_add}" Foreground="White" FontSize="15" VerticalAlignment="Center"/>
                        </StackPanel>

                    </StackPanel>

                    <ListBox x:Name="ui_form_start_ring_list" Grid.Row="2" ItemTemplate="{StaticResource ui_temp_form_start_ring_list}" Background="#00000000" ScrollViewer.HorizontalScrollBarVisibility="Hidden" BorderThickness="0">
                        <ListBox.ItemsPanel>
                            <ItemsPanelTemplate>
                                <StackPanel Orientation="Vertical" IsItemsHost="True" />
                            </ItemsPanelTemplate>
                        </ListBox.ItemsPanel>
                    </ListBox>

                </Grid>

                <!-- 不发行版本标示-->
                <TextBlock x:Name="ui_form_start_secreat_text" Grid.Column="0" Grid.ColumnSpan="3" Width="300" Foreground="white" FontSize="15" TextWrapping="Wrap" TextAlignment="Center" HorizontalAlignment="Right" VerticalAlignment="Bottom"/>

            </Grid>


            <!-- 线路界面-->
            <Grid x:Name="ui_form_line" Width="800" Height="570" Canvas.Top="0" Canvas.Left="-800">

                <Grid.RowDefinitions >
                    <!-- 空白-->
                    <RowDefinition Height="40"/>
                    <!-- 车辆描述-->
                    <RowDefinition Height="*"/>
                    <!-- 空白-->
                    <RowDefinition Height="20"/>

                    <!-- 切换上下行-->
                    <RowDefinition Height="30"/>
                    <!-- 空白-->
                    <RowDefinition Height="5"/>
                    <!-- 线路中的站台列表-->
                    <RowDefinition Height="100"/>
                    <!-- 空白-->
                    <RowDefinition Height="10"/>

                    <!-- 地铁线路中每一个站点中的出口列表-->
                    <RowDefinition Height="100"/>

                    <!-- 空白-->
                    <RowDefinition Height="20"/>

                </Grid.RowDefinitions>
                <Grid.ColumnDefinitions >
                    <ColumnDefinition Width="100"/>
                    <ColumnDefinition Width=" *"/>
                    <ColumnDefinition Width="100"/>
                </Grid.ColumnDefinitions>

                <TextBlock x:Name="ui_form_line_describe" Grid.Column="1" Grid.Row="1" Foreground="white" FontSize="15" TextWrapping="Wrap" Text="Nothing" x:FieldModifier="public"/>

                <!-- 切换上下行-->
                <Grid Grid.Column="1" Grid.Row="3">
                    <Grid.ColumnDefinitions >
                        <ColumnDefinition Width="*"/>
                        <ColumnDefinition Width="auto"/>
                        <ColumnDefinition x:Name="ui_form_line_form_no_down_line_width" Width="auto"/>
                        <ColumnDefinition Width="10"/>
                        <ColumnDefinition x:Name="ui_form_line_form_re_mid_bus_width" Width="auto"/>
                        <ColumnDefinition Width="*"/>
                    </Grid.ColumnDefinitions>

                    <!-- 上行-->
                    <Grid Grid.Column="1" MouseDown="ui_form_line_select_up_line_fx">
                        <Grid.Background >
                            <SolidColorBrush x:Name="ui_form_line_select_up_line" Color="#8f0000ff"/>
                        </Grid.Background>

                        <StackPanel Orientation="Horizontal" VerticalAlignment="Center" HorizontalAlignment="Center" Margin="10,0,10,0">
                            <TextBlock Grid.Row="1" FontSize="15" Foreground="White" TextAlignment="Center" Text="{DynamicResource lang_form_line_up_line_describe}"/>
                            <TextBlock x:Name="ui_form_line_select_up_line_describe" Grid.Row="1" FontSize="15" Foreground="White" TextAlignment="Center" Text="（）"/>
                        </StackPanel>

                        <Rectangle Fill="#00000000" Stroke="White" StrokeThickness="2"/>

                    </Grid>

                    <!-- 下行-->
                    <Grid Grid.Column="2" MouseDown="ui_form_line_select_down_line_fx">
                        <Grid.Background >
                            <SolidColorBrush x:Name="ui_form_line_select_down_line" Color="#00000000"/>
                        </Grid.Background>

                        <StackPanel Orientation="Horizontal" VerticalAlignment="Center" HorizontalAlignment="Center" Margin="10,0,10,0">
                            <TextBlock Grid.Row="1" FontSize="15" Foreground="White" TextAlignment="Center" Text="{DynamicResource lang_form_line_down_line_describe}"/>
                            <TextBlock x:Name="ui_form_line_select_down_line_describe" Grid.Row="1" FontSize="15" Foreground="White" TextAlignment="Center" Text="（）"/>
                        </StackPanel>

                        <Rectangle Fill="#00000000" Stroke="White" Grid.RowSpan="3" StrokeThickness="2"/>

                    </Grid>

                    <!-- 刷新-->
                    <Grid Grid.Column="4">
                        <Grid.ColumnDefinitions >
                            <ColumnDefinition Width="40"/>
                            <ColumnDefinition Width="auto"/>
                        </Grid.ColumnDefinitions>
                        <Grid.RowDefinitions >
                            <RowDefinition Height="*"/>
                            <RowDefinition Height="auto"/>
                            <RowDefinition Height="*"/>
                        </Grid.RowDefinitions>

                        <Grid Grid.Column="0" Grid.RowSpan="3" Margin="10,0,0,0">
                            <Grid.Background >
                                <ImageBrush ImageSource="photo\re.png"/>
                            </Grid.Background>
                        </Grid>

                        <TextBlock x:Name="ui_form_line_re_bus_describe" Grid.Column="1" Grid.Row="1" Foreground="White" Background="#00000000" FontSize="15" Text="{DynamicResource lang_form_line_update_nothing_describe}"/>

                    </Grid>

                </Grid>

                <!-- 车辆含有站台列表 -->
                <!-- 底部的覆盖线-->
                <Grid Grid.Column="1" Grid.Row="5">
                    <Grid.RowDefinitions >
                        <RowDefinition Height="11"/>
                        <RowDefinition Height="4"/>
                        <RowDefinition Height="8"/>
                        <RowDefinition Height="*"/>
                    </Grid.RowDefinitions>

                    <Grid Grid.Row="1" Background="White" />
                </Grid>
                <!-- 真正的列表-->
                <ListBox x:Name="ui_form_line_have_stop_list" ItemTemplate ="{StaticResource ui_temp_form_line_have_stop_list}" Grid.Column="1" Grid.Row="5" Background="#00000000" ScrollViewer.VerticalScrollBarVisibility="Disabled" BorderThickness="0" ScrollViewer.HorizontalScrollBarVisibility="Hidden">
                    <ListBox.ItemsPanel>
                        <ItemsPanelTemplate>
                            <WrapPanel IsItemsHost="True" />
                        </ItemsPanelTemplate>
                    </ListBox.ItemsPanel>
                </ListBox>

                <!-- 地铁线路出口列表 -->
                <ListBox x:Name="ui_form_line_subway_list" ItemTemplate="{StaticResource ui_temp_form_line_subway_list}" Grid.Column="1" Grid.Row="7" Background="#00000000" ScrollViewer.VerticalScrollBarVisibility="Disabled" BorderThickness="0" ScrollViewer.HorizontalScrollBarVisibility="Hidden">
                    <ListBox.ItemsPanel>
                        <ItemsPanelTemplate>
                            <WrapPanel IsItemsHost="True" />
                        </ItemsPanelTemplate>
                    </ListBox.ItemsPanel>
                </ListBox>

            </Grid>

            <!-- 线路界面的控制界面-->
            <Grid x:Name="ui_form_line_contorl" Width="300" Height="570" Canvas.Top="0" Canvas.Left="-300">
                <!-- 背景颜色-->
                <Grid.Background>
                    <SolidColorBrush x:Name="ui_color_1" Color="#8f0000ff"/>
                </Grid.Background>

                <Grid.RowDefinitions >

                    <!-- 标题分页-->
                    <RowDefinition Height="50"/>
                    <!-- 选中时的下滑条-->
                    <RowDefinition Height="2"/>

                    <RowDefinition Height="10"/>
                    <!-- 内容区-->
                    <RowDefinition Height="*"/>
                    <RowDefinition Height="10"/>

                </Grid.RowDefinitions>
                <Grid.ColumnDefinitions >
                    <ColumnDefinition Width=" 20"/>
                    <ColumnDefinition Width=" *"/>
                    <ColumnDefinition Width=" *"/>
                    <ColumnDefinition Width=" 20"/>
                </Grid.ColumnDefinitions>

                <!-- 模板-->
                <Grid Grid.Column="1" Grid.Row="0" MouseDown="ui_function_line_contorl_left_btn">
                    <Grid.ColumnDefinitions >
                        <ColumnDefinition Width="*"/>
                        <ColumnDefinition Width=" auto"/>
                        <ColumnDefinition Width="*"/>
                    </Grid.ColumnDefinitions>
                    <Grid.RowDefinitions >
                        <RowDefinition Height=" *"/>
                        <RowDefinition Height=" auto"/>
                        <RowDefinition Height=" *"/>
                    </Grid.RowDefinitions>

                    <TextBlock Foreground="white" FontSize="15" Grid.Column="1" Grid.Row="1" Text="{DynamicResource lang_form_line_contorl_left_grid_title}"/>
                </Grid>
                <Grid Grid.Column="2" Grid.Row="0" MouseDown="ui_function_line_contorl_right_btn">
                    <Grid.ColumnDefinitions >
                        <ColumnDefinition Width="*"/>
                        <ColumnDefinition Width=" auto"/>
                        <ColumnDefinition Width="*"/>
                    </Grid.ColumnDefinitions>
                    <Grid.RowDefinitions >
                        <RowDefinition Height=" *"/>
                        <RowDefinition Height=" auto"/>
                        <RowDefinition Height=" *"/>
                    </Grid.RowDefinitions>

                    <TextBlock Foreground="white" FontSize="15" Grid.Column="1" Grid.Row="1" Text="{DynamicResource lang_form_line_contorl_right_grid_title}"/>
                </Grid>

                <Grid x:Name="ui_form_line_contorl_left_grid" Grid.Column="1" Grid.Row="1">
                    <Grid.Background >
                        <SolidColorBrush x:Name="ui_form_line_contorl_left_grid_color" Color="#ffffffff"/>
                    </Grid.Background>
                </Grid>
                <Grid x:Name="ui_form_line_contorl_right_grid" Grid.Column="2" Grid.Row="1">
                    <Grid.Background >
                        <SolidColorBrush x:Name="ui_form_line_contorl_right_grid_color" Color="#00000000"/>
                    </Grid.Background>
                </Grid>

                <!-- 承载面板 高=控制窗体高-75 默认495-->
                <Canvas Grid.Column="1" Grid.ColumnSpan="2" Grid.Row="3">

                    <!-- 线路列表-->
                    <Grid x:Name="ui_form_line_contorl_left_form" Canvas.Top="0" Canvas.Left="0" Width="260" Height="495">
                        <Grid.RowDefinitions >
                            <RowDefinition Height="30"/>
                            <RowDefinition Height="10"/>
                            <RowDefinition Height="*"/>
                        </Grid.RowDefinitions>

                        <Grid Grid.Row="0">
                            <Grid.ColumnDefinitions >
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="10"/>
                                <ColumnDefinition Width="30"/>
                            </Grid.ColumnDefinitions>

                            <Grid >
                                <Grid.RowDefinitions >
                                    <RowDefinition Height=" *"/>
                                    <RowDefinition Height=" auto"/>
                                    <RowDefinition Height=" *"/>
                                </Grid.RowDefinitions>

