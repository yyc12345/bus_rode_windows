                                                <DropShadowEffect x:Name="ui_contorl_btn_light_9" BlurRadius="20" Color="#FFffffff" Direction="0" ShadowDepth="0" Opacity="0"/>
                                            </Grid.Effect>
                                            <Grid.Triggers>
                                                <EventTrigger RoutedEvent="MouseEnter">
                                                    <BeginStoryboard >
                                                        <Storyboard >
                                                            <DoubleAnimation Storyboard.TargetName="ui_contorl_btn_light_9" Storyboard.TargetProperty="Opacity" From="0" To="1" Duration="0:0:0.2"/>
                                                        </Storyboard>
                                                    </BeginStoryboard>
                                                </EventTrigger>
                                                <EventTrigger RoutedEvent="MouseLeave">
                                                    <BeginStoryboard >
                                                        <Storyboard >
                                                            <DoubleAnimation Storyboard.TargetName="ui_contorl_btn_light_9" Storyboard.TargetProperty="Opacity" From="1" To="0" Duration="0:0:0.2"/>
                                                        </Storyboard>
                                                    </BeginStoryboard>
                                                </EventTrigger>


                                            </Grid.Triggers>

                                        </Grid>

                                    </Grid>

                                </StackPanel>

                            </Grid>
                        </ScrollViewer>

                    </TabItem>

                </TabControl>

            </Grid>


            <!-- 关于界面-->
            <Grid x:Name="ui_form_about" Width="800" Height="570" Canvas.Top="0" Canvas.Left="-800">
                <Grid.RowDefinitions >
                    <RowDefinition Height="40"/>
                    <RowDefinition Height=" *"/>
                    <RowDefinition Height=" 40"/>
                </Grid.RowDefinitions>
                <Grid.ColumnDefinitions >
                    <ColumnDefinition Width="100"/>
                    <ColumnDefinition Width=" *"/>
                    <ColumnDefinition Width=" 100"/>
                </Grid.ColumnDefinitions>

                <StackPanel Grid.Row="1" Grid.Column="1">

                    <TextBlock x:Name="ui_form_about_describe" Foreground="white" FontSize="15" Grid.Column="1" Grid.Row="1" Text=""/>

                    <StackPanel Orientation="Horizontal">
                        <TextBlock Grid.Row="8" Grid.Column="1" Foreground="White" FontSize="15" Text="开源地址（Github）："/>
                        <TextBlock Grid.Row="8" TextDecorations="Underline"  Grid.Column="1" Foreground="#ff0040ff" FontSize="15" Text="https://github.com/yyc12345/bus_rode_all" MouseDown="ui_form_about_github"/>
                    </StackPanel>

                </StackPanel>

            </Grid>

            <!-- 消息界面-->
            <Grid x:Name="ui_form_message" Width="300" Height="570" Canvas.Top="0" Canvas.Left="800">
                <!-- 背景颜色-->
                <Grid.Background>
                    <SolidColorBrush x:Name="ui_color_3" Color="#8f0000ff"/>
                </Grid.Background>

                <Grid.RowDefinitions >

                    <!-- 标题-->
                    <RowDefinition Height="30"/>
                    <!-- 清空消息-->
                    <RowDefinition Height="30"/>
                    <!-- 选中时的下滑条-->
                    <RowDefinition Height="2"/>

                    <RowDefinition Height="10"/>
                    <!-- 内容区-->
                    <RowDefinition Height="*"/>
                </Grid.RowDefinitions>
                <Grid.ColumnDefinitions >
                    <ColumnDefinition Width=" 20"/>
                    <ColumnDefinition Width=" *"/>
                    <ColumnDefinition Width=" 20"/>
                </Grid.ColumnDefinitions>

                <!-- 标题-->
                <Grid Grid.Row="0" Grid.Column="1" MouseDown="ui_form_message_function">
                    <Grid.ColumnDefinitions >
                        <ColumnDefinition Width=" auto"/>
                        <ColumnDefinition Width=" *"/>
                    </Grid.ColumnDefinitions>
                    <Grid.RowDefinitions >
                        <RowDefinition Height=" *"/>
                        <RowDefinition Height=" auto"/>
                        <RowDefinition Height=" *"/>
                    </Grid.RowDefinitions>

                    <TextBlock Grid.Column="0" Grid.Row="1" Foreground="White" FontSize="17" Text="消息中心"/>
                </Grid>
                <!-- 清空-->
                <Grid x:Name="ui_form_message_clear" Grid.Row="1" Grid.Column="1" MouseDown="ui_form_message_function" Opacity="0">
                    <Grid.ColumnDefinitions >
                        <ColumnDefinition Width=" *"/>
                        <ColumnDefinition Width=" auto"/>
                    </Grid.ColumnDefinitions>
                    <Grid.RowDefinitions >
                        <RowDefinition Height=" *"/>
                        <RowDefinition Height=" auto"/>
                        <RowDefinition Height=" *"/>
                    </Grid.RowDefinitions>

                    <TextBlock Grid.Column="1" Grid.Row="1" Foreground="White" FontSize="15" Text="清空消息"/>
                </Grid>
                <Grid x:Name="ui_form_message_up_line" Grid.Row="2" Grid.Column="0" Grid.ColumnSpan="3" Background="white" Opacity="0"/>

                <Grid x:Name="ui_form_message_no_msg_title" Grid.Row="4" Grid.Column="1">
                    <Grid.ColumnDefinitions >
                        <ColumnDefinition Width=" *"/>
                        <ColumnDefinition Width=" auto"/>
                        <ColumnDefinition Width=" *"/>
                    </Grid.ColumnDefinitions>
                    <Grid.RowDefinitions >
                        <RowDefinition Height=" *"/>
                        <RowDefinition Height=" auto"/>
                        <RowDefinition Height=" *"/>
                    </Grid.RowDefinitions>

                    <TextBlock Grid.Column="1" Grid.Row="1" Foreground="White" FontSize="15" Text="没有消息"/>
                </Grid>

                <ListBox x:Name="ui_form_message_list" Grid.Column="1" Grid.Row="4" ItemTemplate="{StaticResource ui_temp_form_message_list}" Background="#00000000" BorderThickness="0" ScrollViewer.HorizontalScrollBarVisibility="Hidden"/>

            </Grid>

        </Canvas>

    </Grid>
</Window>
